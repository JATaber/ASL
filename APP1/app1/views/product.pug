extend layout
block content
  div.top
  div.container
    div.jumbotron
      h1 Fruit and Vegetable List
      p #[a(href='/add') New Entry]
      ul
        each item in product
          li
            p #{item["name"]} #[a(href='/update/'+item['id']) Update] || #[a(href='/delete/'+item['id']) Delete]
